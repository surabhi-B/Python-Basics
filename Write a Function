def is_leap(year):
    if(year>=1900):
        if(year<=10**5):
            if(year%4==0):
                if(year%100==0):
                    if(year%400==0):
                        leap = True
                    else:
                        leap = False
                if(year%100!=0):
                    leap = True
            else:
                leap = False
        
    # Write your logic here
    
    return leap

